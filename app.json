{
  "expo": {
    "name": "Skate to the Point",
    "slug": "skatetothepoint",
    "description": "Freestyle Skating Registration App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "devClient": true,
      "edgeToEdgeEnabled": true,
      "package": "com.jpking72.skatetothepoint"
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
